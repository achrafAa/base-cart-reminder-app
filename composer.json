{
  "name": "achrafaa/basic-cart-reminder-app",
  "description": "A simple cart reminder app",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Achraf AAmri",
      "email": "achraf.aamri+github-aaa@gmail.com"
    }
  ],
    "autoload": {
        "files": [
            "framework/Helpers/Functions.php"
        ],
        "psr-4": {
          "Achraf\\framework\\": "framework/",
          "App\\": "src/"
        }
    },
  "require-dev": {
    "symfony/var-dumper": "^7.0",
    "squizlabs/php_codesniffer": "^3.7.2",
    "friendsofphp/php-cs-fixer": "^3.40"
  },
  "require": {
    "nikic/fast-route": "^1.3",
    "illuminate/database": "^10.34",
    "vlucas/phpdotenv": "^5.6",
    "nesbot/carbon": "^2.71",
    "symfony/console": "^6.3",
    "fakerphp/faker": "^1.23",
    "monolog/monolog": "^3.5",
    "predis/predis": "^2.2",
    "twig/twig": "^3.0"
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix",
    "docker-up" : "docker compose up -d --build webapp",
    "docker-down" : "docker compose down"
  }
}
